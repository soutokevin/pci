<% if user_signed_in? %>
	<%= link_to('Logout', destroy_user_session_path, method: :delete) %>
<% end %>
<% if @page.name == 'Home Page' %>
	<%= render 'layouts/home', page: @page %>
<% end %>
<% if @page.name == 'Client' %>
	<%= render 'layouts/clients', page: @page %>
<% end %>
<% if @page.name == 'Student' %>
	<%= render 'layouts/students', page: @page %>
<% end %>